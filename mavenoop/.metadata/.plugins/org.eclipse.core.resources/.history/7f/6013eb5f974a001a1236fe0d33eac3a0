package com.mycompany.oopsmaven;

import java.util.ArrayList;

public class Gifts {
    public static ArrayList<Integer> wgt=new ArrayList<Integer>();
    public int getWeight(){
    	int totalweight=0;
    	for(int i=0;i<wgt.size();i++){
    		totalweight+=wgt.get(i);
    	}
    	return totalweight;
    }
	
}
